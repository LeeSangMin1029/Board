<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include ("../partial/header.ejs") %>
  </head>

  <body>
    <%- include ("../partial/navbar.ejs") %> <%- include ("../partial/body.ejs")
    %>
    <div class="container">
      <h4>New Post</h4>
      <form method="POST" novalidate>
        Title : <input type="text" name="title" /><br />
        Body : <input type="text" name="postBody" /><br />
        <a class="btn btn-primary mb-2" href="/posts" role="button">Back</a>
        <input
          type="submit"
          class="btn btn-primary mb-2"
          value="create"
          formaction="/posts"
        />
      </form>
    </div>
  </body>
</html>
